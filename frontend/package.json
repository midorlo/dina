{
  "name": "mi",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist src/typed-router.d.ts src/auto-imports.d.ts src/components.d.ts",
    "dev": "vite",
    "prebuild": "npm run clean",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix --quiet",
    "lint:check": "eslint . --quiet",
    "format:prettier": "prettier --write . --log-level warn",
    "format": "run-s -s format:prettier lint",
    "build:ci": "run-s -s lint:check build"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "@sentry/vue": "^10.5.0",
    "cropperjs": "^2.0.1",
    "dompurify": "^3.1.7",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.17",
    "vuetify": "^3.9.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-vuetify": "^4.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vue-scoped-css": "^2.11.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-layouts-next": "^1.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
